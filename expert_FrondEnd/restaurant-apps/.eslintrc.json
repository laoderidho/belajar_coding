{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["webpack.common.js", "webpack.dev.js", "webpack.prod.js"],
    "rules": {
        "arrow-parens": ["error", "as-needed"],
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
